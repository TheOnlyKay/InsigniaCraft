{
  "model": {
    "type": "select",
    "property": "component",
    "component": "custom_name",
    "fallback": {
        "type": "model",
        "model": "minecraft:block/carved_pumpkin"
    },
    "cases": [
        {
            "when": "Menace",
            "model": {
                "type": "model",
                "model": "mccit:item/hats/phantom_menace"
            }
        },
        {
          "when": "Mask",
          "model": {
              "type": "model",
              "model": "mccit:item/hats/phantom_mask"
          }
        }
    ]
  }
}